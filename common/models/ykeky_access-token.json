{
  "name": "YKEKY_AccessToken",
  "base": "AccessToken",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "ttl": {
      "type": "number",
      "ttl": true,
      "default": 86400,
      "description": "time to live in day (1 day by default)"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "idName": "id",
      "polymorphic": {
        "idType": "string",
        "foreignKey": "userId",
        "discriminator": "principalType"
      }
    }
  },
  "acls": [],
  "methods": {}
}
